{
  "version": 2,
  "projects": {
    "slim-mom": {
      "src": "slim-mom/",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    },
    "slim-mom-backend": {
      "src": "slim-mom-backend/",
      "use": "@vercel/node"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "slim-mom-backend/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "slim-mom/$1"
    }
  ]
}
